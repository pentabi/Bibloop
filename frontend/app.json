{
  "expo": {
    "name": "Bibloop",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bibloop-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#007AFF",
      "dark": {
        "image": "./assets/images/splash-dark.png",
        "backgroundColor": "#000000"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "appleId": "tabitosatoh@gmail.com",
      "ascAppId": "6751510418",
      "appleTeamId": "5Y25M32JP9",
      "supportsTablet": true,
      "bundleIdentifier": "com.tabitosatoh.Bibloop",
      "associatedDomains": [
        "applinks:myapp"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take profile pictures and share photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select and share photos.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save photos to your photo library."
      }
    },
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "myapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "package": "com.anonymous.frontend",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#007AFF",
          "image": "./assets/images/splash.png",
          "dark": {
            "image": "./assets/images/splash-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "カメラロールを使う許可が必要です",
          "cameraPermission": "カメラを使う許可が必要です"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5763bc7a-7d19-4699-8dca-a5a8ed97f09c"
      }
    },
    "owner": "tabito"
  }
}
